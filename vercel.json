{
  "version": 2,
  "rewrites": [
    {
      "source": "/tuner",
      "destination": "/tuner.html"
    },
    {
      "source": "/metronome",
      "destination": "/metronome.html"
    },
    {
      "source": "/chords",
      "destination": "/chords.html"
    },
    {
      "source": "/timer",
      "destination": "/timer.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

